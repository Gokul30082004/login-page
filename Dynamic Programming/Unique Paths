import java.util.* ;
import java.io.*; 
public class Solution {
	public static int uniquePaths(int m, int n) {
		int dp[]=new int[n];
		Arrays.fill(dp,0);
		dp[0]=1;
		for(int i=0;i<m;i++){
			int temp[]=new int[n];
			for(int j=0;j<n;j++){
				if(j-1>=0)
				temp[j]=dp[j]+temp[j-1];
				else
				temp[j]=dp[j];
			}
			dp=temp;
		}
		return dp[n-1];
	}
}
